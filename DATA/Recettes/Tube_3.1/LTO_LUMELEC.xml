</step>
<step n="109" titre="exces pression sous O2" saut="36" >
	<vanne n="15" /> <vanne n="Ferm. Purges" />
</step>
<step n="110" titre="exces pression pendant depot" saut="36" >
	<vanne n="15" /> <vanne n="Ferm. Purges" />
